{
  "name": "kawaiicode",
  "id": "ZakyEvenso12",
  "version": "2.0.6",
  "publisher": "ZakyEvenso12",
  "displayName": "CodersLar Pro",
  "description": "CodersLar Pro - AI-powered CLI extension for developers.",
  "bin": {
    "coders-solid": "./cli/coders-solid.js",
    "coders-delete": "./cli/delete-solid.js",
    "coders-crud": "./cli/crud-installer.js"
  },
  "tags": [
    "codersLar",
    "codersHtml",
    "laravel",
    "AI",
    "snippets"
  ],
  "engines": {
    "vscode": "1.97.1"
  },
  "categories": [
    "Programming Languages",
    "AI",
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html-snippets.code-snippets.json"
      },
      {
        "language": "php",
        "path": "./snippets/laravel-snippets.code-snippets.json"
      },
      {
        "language": "php",
        "path": "./snippets/php-snippets.code-snippets.json"
      },
      {
        "language": "java",
        "path": "./snippets/java-snippets.code-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript-snippets.code-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/vue-snippets.code-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/ReactNative-snippets.code-snippets.json"
      }
    ],
    "commands": [
      {
        "command": "extension.codersSolid",
        "title": "Run Coders Solid CLI"
      },
      {
        "command": "extension.codersDelete",
        "title": "Run Coders Delete CLI"
      },
      {
        "command": "extension.codersCrud",
        "title": "Run Coders CRUD CLI"
      }
    ]
  },
  "activationEvents": [
    "onCommand:extension.codersSolid",
    "onCommand:extension.codersDelete",
    "onCommand:extension.codersCrud"
  ],
  "scripts": {
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZakyEvenso12/CodersLar-Pro"
  },
  "icon": "Image/CodersLar.png",
  "keywords": [
    "codersLar",
    "php",
    "laravel",
    "AI",
    "javascript",
    "Vue",
    "snippets"
  ],
  "author": "Zaky Evenso",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.8.4",
    "@types/vscode": "^1.95.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "ora": "^8.1.1"
  }
}
