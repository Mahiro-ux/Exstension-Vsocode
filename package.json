    "Programming Languages",
    "AI",
    "Snippets"
  ],
  "contributes": {
    "snippets": [
        {
            "language": "html",
            "path": "./.vscode/snippets.code-snippets"
        },
        {
            "language": "php",
            "path": "./.vscode/laravel-snippets.code-snippets"
        },
        {
            "language": "php",
            "path": "./snippets/php-snippets.code-snippets"
        },
        {
            "language": "java",
            "path": "./.vscode/java-snippets.code-snippets"
        },
        {
            "language": "javascript",
            "path": "./.vscode/javascript-snippets.code-snippets"
        },
        {
            "language": "javascript",
            "path": "./.vscode/vue-snippets.code-snippets"
        },
        {
            "language": "javascript",
            "path": "./.vscode/react-native-snippets.code-snippets"
        }
    ]
},
  "scripts": {
