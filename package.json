  "description": "CodersLang - Shortcuts for VS Code Provides various Laravel, php, vue, html, React.java, javascript language shortcuts with simple codes",
  "bin": {
    "coders-solid": "./cli/coders-solid.js",
    "coders-delete": "./cli/delete-solid.js"
  },
  "tags": [
    "codersLar",
    "codersHtml"
  ],
  "engines": {
    "vscode": "^1.9.4"
  },
